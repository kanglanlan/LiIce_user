<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_yin_hang_ka"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical"
    tools:context="com.meida.liice.MyYinHangKaActivity">

    <include layout="@layout/title" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@color/xian"
        android:dividerHeight="1px"
        android:id="@+id/listview"
        android:listSelector="@color/white"></ListView>

    <TextView
        android:id="@+id/tv_zhuaneng"
        style="@style/Font14"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_margin="15dp"
        android:background="@drawable/baikuang"
        android:gravity="center"
        android:text="+添加银行卡" />
</LinearLayout>
